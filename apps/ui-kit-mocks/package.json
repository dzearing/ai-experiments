{
  "name": "@claude-flow/ui-kit-mocks",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageType": "react-app",
  "scripts": {
    "build": "repo-scripts build",
    "build:storybook": "repo-scripts build-storybook",
    "clean": "repo-scripts clean",
    "dev": "repo-scripts dev",
    "dev:storybook": "repo-scripts dev-storybook",
    "dev:storybook:wsl": "repo-scripts dev-storybook-wsl",
    "dev:vite-only": "repo-scripts dev-vite-only",
    "dev:wsl": "repo-scripts dev-wsl",
    "lint": "repo-scripts lint",
    "lint:fix": "repo-scripts lint-fix",
    "postinstall": "node -e \"const fs = require('fs'), path = require('path'); const linkPath = path.join(__dirname, 'public/themes'); const targetPath = path.join(__dirname, '../../packages/ui-kit-legacy/dist/themes'); try { fs.unlinkSync(linkPath); } catch {} try { fs.symlinkSync(path.relative(path.dirname(linkPath), targetPath), linkPath); } catch (e) { console.log('Theme symlink creation failed:', e.message); }\"",
    "preview": "repo-scripts preview",
    "test": "repo-scripts test",
    "test:e2e": "repo-scripts test-e2e",
    "test:e2e:ui": "repo-scripts test-e2e-ui",
    "test:watch": "repo-scripts test-watch",
    "typecheck": "repo-scripts typecheck",
    "format": "repo-scripts format",
    "format:check": "repo-scripts format-check"
  },
  "dependencies": {
    "@claude-flow/ui-kit-legacy": "workspace:*",
    "@claude-flow/ui-kit-react": "workspace:*",
    "@claude-flow/ui-kit-icons": "workspace:*",
    "classnames": "^2.5.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3"
  },
  "devDependencies": {
    "@claude-flow/repo-scripts": "workspace:*",
    "@claude-flow/tsconfig": "workspace:*",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6"
  }
}
